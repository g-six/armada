{
  "extends": "../tsconfig.json",
  "include": ["functions", "generics", "libs", "services"],
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "generics/*": ["./generics/*"],
      "libs/*": ["./libs/*"],
      "services/*": ["./services/*"],
      "*": ["./*"]
    }
  }
}
